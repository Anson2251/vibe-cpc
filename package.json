{
  "name": "caie-pseudocode-interpreter",
  "type": "module",
  "version": "1.0.0",
  "description": "A TypeScript interpreter for the CAIE pseudocode language",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "caie-pseudocode": "dist/cli/index.js"
  },
  "scripts": {
    "build": "pnpm clean; node build-cli.mjs --target node",
    "test": "pnpx jest",
    "lint": "eslint src/**/*.ts tests/**/*.ts --max-warnings=0",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "lint:ci": "eslint src/**/*.ts tests/**/*.ts --max-warnings=0 --format=stylish",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "caie",
    "pseudocode",
    "interpreter",
    "typescript",
    "compiler"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "esbuild": "^0.25.9"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
